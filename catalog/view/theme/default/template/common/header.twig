  <div class="modal fade" id="login" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h6 class="h2 text-center title text-uppercase">Заповніть форму</h6>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">
          <div class="login-form-container">
            <form action="{{ action }}" method="post" enctype="multipart/form-data">
              <div class="form-group">
{#                <input type="text" name="email" value="{{ email }}" placeholder="{{ entry_email }}" id="input-email" class="form-control" />#}
                <input type="text" name="email" value="{{ email }}" placeholder="Email" id="input-email" class="form-control" />
              </div>
              <div class="form-group">
{#                <input type="password" name="password" value="{{ password }}" placeholder="{{ entry_password }}" id="input-password" class="form-control" />#}
                <input type="password" name="password" value="{{ password }}" placeholder="Пароль" id="input-password" class="form-control" />
{#                <a href="{{ forgotten }}">{{ text_forgotten }}</a>#}
              </div>
              <div class="form-group">
                <div class="checkbox text-right">
                  <label>
                    <input type="checkbox"/>
                    <span>
                           Запам’ятати мене
                        </span>

                  </label>
                </div>
              </div>
              <div class="text-right">
                <a  href="#" data-toggle="modal" data-target="#forgot">Забули пароль?</a>
              </div>

              <input type="submit" value="{{ button_login }}" class="btn btn_secondary w-100 text-uppercase" />
              <a href="#" class="btn btn_tertiary w-100" data-toggle="modal" data-target="#register">Реєстрація</a>
              {% if redirect %}
                <input type="hidden" name="redirect" value="{{ redirect }}" />
              {% endif %}
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="modal fade" id="forgot" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h6 class="h2 title text-center text-uppercase">ВВедіть Email, який ви вказали при реєстрації</h6>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">
          <div class="wysiwyg text-center">
            <p>ВВедіть Email, який ви вказали при реєстрації</p>
          </div>
          <div class="login-form-container">
            <form action="{{ action }}" method="post" enctype="multipart/form-data">
              <fieldset>
                <div class="form-group required">
{#                    <input type="text" name="email" value="{{ email }}" placeholder="{{ entry_email }}" id="input-email" class="form-control" />#}
                    <input type="text" name="email" value="{{ email }}" placeholder="ВВедіть Email, який ви вказали при реєстрації" id="input-email" class="form-control" />
                </div>
              </fieldset>
              <div class="buttons">
                <input type="submit" value="{{ button_continue }}" class="btn btn_secondary w-100" />
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="modal fade" id="register" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h6 class="h2 title text-center text-uppercase">Заповніть форму</h6>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">
          <div class="login-form-container">
            <form action="{{ action }}" method="post" enctype="multipart/form-data">
              <fieldset id="account">
                <div class="form-group required">

{#                    <input type="text" name="firstname" value="{{ firstname }}" placeholder="{{ entry_firstname }}" id="input-firstname" class="form-control" />#}
                    <input type="text" name="firstname" value="{{ firstname }}" placeholder="Ім’я" id="input-firstname" class="form-control" />
                    {% if error_firstname %}
                      <div class="text-danger">{{ error_firstname }}</div>
                    {% endif %}
                </div>
                <div class="form-group required">
{#                    <input type="email" name="email" value="{{ email }}" placeholder="{{ entry_email }}" id="input-email" class="form-control" />#}
                    <input type="email" name="email" value="{{ email }}" placeholder="Email" id="input-email" class="form-control" />
                    {% if error_email %}
                      <div class="text-danger">{{ error_email }}</div>
                    {% endif %}
                </div>
                <div class="form-group required">
{#                    <input type="tel" name="telephone" value="{{ telephone }}" placeholder="{{ entry_telephone }}" id="input-telephone" class="form-control" />#}
                    <input type="tel" name="telephone" value="{{ telephone }}" placeholder="Номер телефону" id="input-telephone" class="form-control tel" />
                    {% if error_telephone %}
                      <div class="text-danger">{{ error_telephone }}</div>
                    {% endif %}
                </div>
                <div class="form-group required">
{#                    <input type="password" name="password" value="{{ password }}" placeholder="{{ entry_password }}" id="input-password" class="form-control" />#}
                    <input type="password" name="password" value="{{ password }}" placeholder="Введіть пароль" id="input-password" class="form-control" />
                    {% if error_password %}
                      <div class="text-danger">{{ error_password }}</div>
                    {% endif %}
                </div>
              </fieldset>

              <div class="buttons">
{#                <input type="submit" value="{{ button_continue }}" class="btn btn_secondary w-100 text-uppercase" />#}
                <input type="submit" value="ЗАРЕЄСТРУВАТИСЯ" class="btn btn_secondary w-100 text-uppercase" />
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>

